{
  "name": "@talend/module-to-cdn",
  "version": "3.4.0",
  "description": "Get cdn config from npm module name",
  "license": "MIT",
  "repository": "toutpt/module-to-cdn",
  "author": {
    "name": "Thomas Sileghem",
    "email": "th.sileghem@gmail.com",
    "url": "mastilver.com"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "xo && ava -v"
  },
  "files": [
    "index.js",
    "url.js",
    "modules.json"
  ],
  "keywords": [
    ""
  ],
  "dependencies": {
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "axios": "^0.20.0",
    "axios-retry": "^3.1.8",
    "execa": "^4.0.0",
    "mkdirp": "^1.0.4",
    "react": "16.0.0",
    "xo": "^0.33.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "ignores": [
      "**/coverage/**"
    ],
    "rules": {
      "ava/no-ignored-test-files": "off",
      "eqeqeq": "off",
      "no-eq-null": "off",
      "node/no-unsupported-features/es-syntax": "off",
      "unicorn/no-reduce": "off"
    }
  }
}
